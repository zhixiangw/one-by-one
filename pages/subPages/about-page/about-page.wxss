.container {
  width: 100%;
  height: 100vh;
  background: #f7f7f7;
  overflow: auto;
  display: flex;
  justify-content: space-between;
  flex-direction: column;
  padding: 40rpx 0;
  box-sizing: border-box;
  text-align: center;
  color: #666;
}

.logo{
  width: 200rpx;
  margin: 20rpx auto;
}

.logo image {
  width: 200rpx;
  height: 200rpx;
  border-radius: 100%;
}

.title{
  font-size: 38rpx;
  color: #333;
}

.sub-title{
  font-size: 32rpx;
  color: #666;
  margin: 30rpx 0;
}

.version{
  color: #666;
}

.phone{
  margin: 10rpx 0;
}

.user-tip{
  text-decoration: underline;
}