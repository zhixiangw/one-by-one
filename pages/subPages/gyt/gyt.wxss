page {
  background-color: #f4f4f4;
}
swiper {
  height: 560rpx;
  margin-bottom: 40rpx;
}
.swiper-item {
  padding-top: 100rpx;
  height: 300rpx;
  background-color: darkgoldenrod;
}
.card-item {
  width: 85%;
  box-sizing: border-box;
  height: 400rpx;
  padding-top: 20rpx;
  background: #fff;
  margin: 0 auto;
  border-radius: 12rpx;
  box-shadow: 0 2px 17px 0 #EFF2F9;
}

.rights, .protocol {
  width: 90%;
  margin: 0 auto;
  border-radius: 12rpx;
  background-color: #fff;
  padding: 40rpx 30rpx;
  box-sizing: border-box;
  box-shadow: 0 2px 17px 0 #EFF2F9;
}

.protocol {
  color: #666;
  padding: 50rpx 30rpx;
  margin-top: 40rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}



.empty {
  position: absolute;
  height: 100%;
  width: 100%;
  text-align: center;
  background-color: #f4f4f4;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}

.empty .gyt-empty {
  display: block;
  width: 200rpx;
  height: 200rpx;
  margin-top: -100rpx;
}

.empty .desc {
  font-size: 28rpx;
  color: #333;
  margin: 40rpx auto;
}

.empty .go-bind {
  width: 200rpx;
  height: 50rpx;
  line-height: 50rpx;
  color: #666;
  font-size: 24rpx;
  border: 1rpx dashed #666;
  border-radius: 8rpx;
  background-color: #fff;
}